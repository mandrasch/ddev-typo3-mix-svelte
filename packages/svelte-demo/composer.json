{
  "name": "kean/svelte-demo",
  "type": "typo3-cms-extension",
  "description": "Demo project for svelte integration into TYPO3",
  "authors": [
    {
      "name": "Markus Ketterer",
      "role": "Developer"
    },
    {
      "name": "Matthias Andrasch",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "KeAn\\SvelteDemo\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/svelte-demo": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/public",
      "extension-key": "svelte-demo"
    }
  }
}
